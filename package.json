{
  "name": "empty-project-template",
  "main": "node_modules/expo/AppEntry.js",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__test__/**",
      "!**/coverage/**",
      "!**/setupTests.js"
    ],
    "preset": "react-native",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./setupTests.js"
    ],
    "verbose": true
  },
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "jest"
  },
  "dependencies": {
    "expo": "^30.0.1",
    "react": "16.3.1",
    "react-dom": "^16.5.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz"
  },
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.6.0",
    "jsdom": "^12.2.0",
    "react-native-mock": "^0.3.1",
    "react-native-mock-render": "^0.1.2",
    "react-test-renderer": "^16.5.2"
  }
}
